{
  "name": "jekyll-markdown-parser",
  "description": "A Jekyll Markdown parser using TypeScript",
  "version": "1.0.1",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net/"
  },
  "babel": {
    "presets": [
      "es2015",
      "power-assert"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/jekyll-markdown-parser/issues"
  },
  "dependencies": {
    "js-yaml": "3.6.1",
    "marked": "0.3.6"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.9.1",
    "@types/marked": "^0.3.0",
    "@types/node": "^8.0.34",
    "@types/power-assert": "^1.4.29",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^2.3.6",
    "babel-cli": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-power-assert": "^1.0.0",
    "beater": "^5.0.1",
    "cpx": "1.5.0",
    "npm-run-all": "3.1.0",
    "power-assert": "^1.4.4",
    "proxyquire": "1.7.10",
    "rimraf": "2.5.4",
    "sinon": "1.17.5",
    "typescript": "^2.5.3",
    "watch": "0.19.2"
  },
  "files": [
    "lib/"
  ],
  "homepage": "https://github.com/bouzuya/jekyll-markdown-parser#readme",
  "keywords": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/jekyll-markdown-parser.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:ts' 'build:es2015' 'build:lib'",
    "build:es2015": "babel --quiet .tmp/es2015/ -d .tmp/es5/",
    "build:lib": "cpx '.tmp/es5/src/**/*.{js,d.ts}' 'lib/'",
    "build:ts": "tsc",
    "clean": "rimraf .tmp/ lib/",
    "prepublish": "npm-run-all -s 'clean' 'build'",
    "start": "node ./lib/index.js",
    "test": "node .tmp/es5/test/index",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2015": "npm run 'build:es2015' -- --watch",
    "watch:test": "watch --wait 2 'npm test' '.tmp/es5/test/'",
    "watch:ts": "npm run 'build:ts' -- --watch"
  },
  "typings": "lib/index.d.ts"
}
